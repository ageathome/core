{
  "name": "Age@Home",
  "description": "Cognitive elder -care and -support",
  "url": "https://github.com/ageathome",
  "slug": "ageathome",
  "image": "dcmartin/{arch}-addon-ageathome",
  "stage": "experimental",
  "version": "0.0.2",
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "hassio_api": false,
  "hassio_role": "default",
  "homeassistant_api": false,
  "host_network": true,
  "ingress": true,
  "ingress_port": 7999,
  "startup": "services",
  "timeout": 300,
  "tmpfs": true,
  "audio": false,
  "video": true,
  "webui": "http://[HOST]:[PORT:7999]/",
  "boot": "auto",
  "environment": {
    "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
  },
  "map": [
    "share:rw",
    "config:rw"
  ],
  "options": {
    "log_level": "info",
    "group": "",
    "device": "",
    "client": "",
    "timezone": "",
    "latitude": "",
    "longitude": "",
    "mqtt": {
      "host": "!secret mqtt-broker",
      "password": "!secret mqtt-password",
      "port": "!secret mqtt-port",
      "username": "!secret mqtt-username"
    },
    "overview": {},
    "w3w": {
      "words": "///what.three.words",
      "apikey": "APIKEY4U"
    }
  },
  "panel_admin": true,
  "panel_icon": "mdi:home-heart",
  "panel_title": "Age@Home",
  "ports": {
    "7999/tcp": 7999
  },
  "ports_description": {
    "7999/tcp": "API"
  },
  "privileged": [
    "SYS_ADMIN"
  ],
  "reload": true,
  "runtime": {
    "io.containerd.runc.v2": false,
    "io.containerd.runtime.v1.linux": false,
    "nvidia": true,
    "runc": true
  },
  "schema": {
    "elevation": "int?",
    "log_file": "str?",
    "log_level": "str",
    "log_type": "str?",
    "reload": "bool?",
    "router_name": "str?",
    "username": "str?",
    "password": "str?",
    "person": {
      "user": "str?"
    },
    "w3w": {
      "words": "str?",
      "apikey": "str?",
      "email": "str?"
    },
    "mqtt": {
      "host": "str?",
      "password": "str?",
      "port": "port?",
      "username": "str?"
    },
    "overview": {
      "apikey": "str?",
      "image": "str?",
      "mode": "list(local|hybrid|roadmap|satellite|terrain)?",
      "zoom": "int(10,22)?"
    }
  }
}
