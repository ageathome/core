{
  "version": "0.7.22",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ],
  "audio": false,
  "boot": "auto",
  "description": "Cognitive elder -care and -support",
  "environment": {
    "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
  },
  "hassio_api": false,
  "hassio_role": "default",
  "homeassistant_api": true,
  "host_network": true,
  "image": "dcmartin/{arch}-addon-ageathome",
  "ingress": true,
  "ingress_port": 7999,
  "init": false,
  "map": [
    "share:rw",
    "config:rw"
  ],
  "name": "Age@Home",
  "options": {
    "site": "My House",
    "roles": {
      "person": "none",
      "primary": "none",
      "secondary": "none",
      "tertiary": "none"
    },
    "mqtt": {
      "host": "core-mosquitto",
      "password": "password",
      "port": 1883,
      "username": "username"
    },
    "overview": {
      "apikey": "",
      "image": "overview.jpg",
      "mode": "local",
      "zoom": 18
    },
    "uptimerobot_rssurl": "",
    "timezone": "America/Los_Angeles",
    "wifi": {
      "ssid": "",
      "password": ""
    },
    "w3w": {
      "apikey": "APIKEY4U",
      "words": "///what.three.words"
    },
    "log_level": "info"
  },
  "panel_admin": true,
  "panel_icon": "mdi:home-heart",
  "panel_title": "Age@Home",
  "ports": {
    "7999/tcp": 7999
  },
  "ports_description": {
    "7999/tcp": "API"
  },
  "privileged": [
    "SYS_ADMIN"
  ],
  "runtime": {
    "io.containerd.runc.v2": false,
    "io.containerd.runtime.v1.linux": false,
    "nvidia": true,
    "runc": true
  },
  "schema": {
    "site": "str?",
    "client": "str?",
    "device": "str?",
    "elevation": "int?",
    "group": "str?",
    "latitude": "float?",
    "log_level": "str",
    "longitude": "float?",
    "mqtt": {
      "host": "str?",
      "password": "str?",
      "port": "int?",
      "username": "str?"
    },
    "overview": {
      "apikey": "str?",
      "image": "str?",
      "mode": "list(local|hybrid|roadmap|satellite|terrain)?",
      "zoom": "int(10,22)?"
    },
    "roles": {
      "person": "str?",
      "primary": "str?",
      "secondary": "str?",
      "tertiary": "str?"
    },
    "reload": "bool?",
    "timezone": "str?",
    "uptimerobot_rssurl": "str?",
    "unit_system": "list(imperial|metric)?",
    "wifi": {
      "ssid": "str?",
      "password": "password?"
    },
    "w3w": {
      "apikey": "str?",
      "words": "str?"
    }
  },
  "services": [
    "mqtt:want"
  ],
  "slug": "ageathome",
  "stage": "stable",
  "startup": "initialize",
  "timeout": 300,
  "tmpfs": true,
  "url": "https://github.com/ageathome",
  "video": false,
  "webui": "http://[HOST]:[PORT:7999]/"
}
